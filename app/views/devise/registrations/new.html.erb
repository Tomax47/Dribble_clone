<section class="signup-div">
  <div class="left-signup-part">
    <div class="back-icon-div" id="back-icon-div">
      <img id="back-icon" src="data:image/png;base64,">
    </div>
  </div>

  <div class="right-signup-part">
    <div class="signup-container">
      <h2 class="signup-phrase">Sign up to Dribbble</h2>

      <div class="google-signup-dev" id="google-sing-up-div">
        <img class="google-icon" src="data:image/png;base64,">
        <%= link_to "Sign in with google", "#{}", class: "google-sign-up-btn"%>
      </div>

      <div class="separator-phrase-div" id="separator-div">
        <hr class="line-separator">
        <p class="separator-phrase"> or </p>
        <hr class="line-separator">
      </div>

      <div class="email-signup-dev">
        <button class="email-sign-up-btn" id="sign-up-form-btn">Continue with email</button>
      </div>

      <div class="signup-form-div">
        <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= f.error_notification %>

          <div class="sign-up-form-inputs" id="sign-up-from-inputs">

            <div class="name-div">
              <label class="form-label">Name</label>
              <div class="name-input">
                <%= f.input_field :name,
                            required: true,
                            label: false,
                            class: "input-field" %>
              </div>
            </div>

            <div class="email-div">
              <label class="form-label">Username or Email</label>
              <div class="email-input">
                <%= f.email_field :email,
                                  required: false,
                                  autofocus: true,
                                  input_html: { autocomplete: "email" },
                                  class: "input-field" %>
              </div>
            </div>

            <div class="enter-password-div">
              <label class="form-label">Password</label>
              <div class="password-input">
                <%= f.password_field :password,
                                     required: false,
                                     hint: ("#{@minimum_password_length} characters minimum" if @minimum_password_length),
                                     label: false,
                                     input_html: { autocomplete: "new-password" },
                                     class: "input-field"%>
              </div>
            </div>

            <div class="confirm-password-div">
              <label class="form-label">Confirm Password</label>
              <div class="confirm-password-input">
                <%= f.password_field :password,
                                     required: false,
                                     label: false,
                                     input_html: { autocomplete: "new-password" },
                                     class: "input-field"%>
              </div>
            </div>

          </div>

          <div class="sign-up-form-actions" id="registration-btn-div">
            <%= f.button :submit, "Create Account", class: "sign-up-btn" %>
          </div>
        <% end %>

        <div class="agreement-phrase-div" id="agreement-phrase">
          <p class="agreement-phrase">
            By creating an account you agree with our
            <span class="terms"><a class="terms-link" href="#"> Terms of Service</a></span>,
            <span class="privacy"><a class="privacy-link" href="#"> Privacy Policy</a></span>, and our default
            <span class="notification"><a class="notification-link" href="#"> Notification Settings</a></span>.
          </p>
        </div>
        <%= render "devise/shared/links" %>
      </div>

    </div>
  </div>
</section>

<%= javascript_pack_tag 'devise' %>